<form action="/military_branches" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new military_branch:</p>
  Name:<input type='text' name='military_branch[name]'/><br/>
  Sea Based:<%=check_box_tag 'military_branch[sea_based]'%><br/>
  Year Established:<input type='text' name='military_branch[year_established]'/><br/>
  
  <input type='submit' name='submit'/>
</form>   